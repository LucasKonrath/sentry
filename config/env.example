# Environment Configuration
GITHUB_TOKEN=your_github_token_here
OPENAI_API_KEY=your_openai_api_key_here

# Coverage Settings
COVERAGE_THRESHOLD=80
MIN_COVERAGE_INCREASE=5

# GitHub Settings
DEFAULT_BRANCH=main
PR_BRANCH_PREFIX=auto-tests/
PR_TITLE_PREFIX="[Auto] Add unit tests to improve coverage"

# LLM Settings
LLM_MODEL=gpt-4
MAX_TOKENS=4000
TEMPERATURE=0.2

# Analysis Settings
SUPPORTED_LANGUAGES=python,javascript,typescript,java
EXCLUDE_PATTERNS=*.pyc,__pycache__,node_modules,*.git

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)
